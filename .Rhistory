library(MASS)
library(bootstrap)
corhat<- cor(law$LSAT,law$GPA)
n<- nrow(law)
corjack<- 1:n
for (i in 1:n) {
LAST<- law$LSAT[-i]
GPA<- law$GPA[-i]
corjack[i]<- cor(LAST,GPA)
}
corbias<- (n-1)*(mean(corjack)-corhat)
se<- sqrt((n-1)*mean((corjack-mean(corjack))^2))
cat("This is jacknife's bias and standard error:",corbias,se)
