f <- function(x,i){
t<- x[i]
12/sum(t)
}
air<- as.vector(aircondit$hours)
obj<- boot(data = air,statistic = f,R= 2000)
obj
print(boot.ci(obj,type = c("basic","norm","perc","bca")))
